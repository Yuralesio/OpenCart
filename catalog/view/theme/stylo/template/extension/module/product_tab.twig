<section class="grp-tab">
  <div id="tabs-{{ module }}" class="htabs clearfix">
  {% if featured_products  or  bestseller_products  or  latest_products  or  special_products %}
    <div class="heading_obj text-center col-xs-12">
      <h2><span class="left"></span>{{heading_title}}<span class="right"></span></h2>
      <div class="obj col-sm-offset-3 col-sm-6 col-xs-12">
        <span><img src="image/custom/head.png" alt="head" /></span>
      </div>
    </div>
  <ul class="nav nav-tabs list-unstyled text-center clearfix col-xs-12">
  {% if featured_products %}
    <li class="">
    <a href="#tab-featured-{{ module }}" data-toggle="tab" class="selected"><h4>{{ tab_featured }}</h4></a></li>
    {% endif %}
    
    {% if bestseller_products %}
    <li>
    <a href="#tab-bestseller-{{ module }}" data-toggle="tab"><h4>{{ tab_bestseller }}</h4></a></li>
    {% endif %}
    
    {% if latest_products %}
    <li>
    <a href="#tab-latest-{{ module }}" data-toggle="tab"><h4>{{ tab_latest }}</h4></a></li>
    {% endif %}
    
    {% if special_products %}
    <li>
    <a href="#tab-special-{{ module }}" data-toggle="tab"><h4>{{ tab_special }}</h4></a></li>
    {% endif %}
  </ul>
  {% endif %}
 </div>
 
 
 <div class="tab-content home-h-tabs clearfix">
{% if latest_products %}
 <div id="tab-latest-{{ module }}" class="tab-content product_carousel">
    <div id="hor_latest" class="box-content ltst cmn-slide product_carousel owl-carousel">
      {% for product in latest_products %}       
      <div class="product-block product-col">
          <div class="product-thumb transition">

            <div class="image">
            <a href="{{ product.href }}"><img src="{{ product.thumb }}" alt="{{ product.name }}" title="{{ product.name }}" class="img-responsive" /></a>
            </div>
            <div class="caption">
              <div class="product-meta">
                <div class="product-meta-inner">


                  <div class="left">
                    <h4><a href="{{ product.href }}">{{ product.name }}</a></h4>
                    <hr/>
                    <div class="hidden rating">
                      {% for i in 1..5 %}
                      {% if product.rating < i %}
                      <span class="fa fa-stack"><i class="fa fa-star-o fa-stack-2x"></i></span>
                      {% else %}
                      <span class="fa fa-stack"><i class="fa fa-star fa-stack-2x"></i><i class="fa fa-star-o fa-stack-2x"></i></span>
                      {% endif %}
                      {% endfor %}
                    </div>        
              {% if product.price %}
              <p class="price pull-left">
                {% if not product.special %}
                {{ product.price }}
                {% else %}
                <span class="price-new">{{ product.special }}</span>
                <span class="price-old">{{ product.price }}</span>
                {% endif %}
                 {% if product.tax %}
          <span class="price-tax">{{ text_tax }} {{ product.tax }}</span>
          {% endif %}
              </p>
               {% endif %}
              <div class="cart-add pull-right">
                <button type="button" class="btn" data-toggle="tooltip" title="{{ button_cart }}" onclick="cart.add('{{ product.product_id }}');"><i class="fa fa-cart-plus" aria-hidden="true"></i>&nbsp;<!-- {{ button_cart }} --></button>
              </div>
              </div>
      

            <div class="button-group">              
              <button type="button" class="btn btn-outline-inverse wishlist" data-toggle="tooltip" title="{{ button_wishlist }}" onclick="wishlist.add('{{ product.product_id }}');"><i class="fa fa-heart"></i></button>
              <button type="button" class="btn btn-outline-inverse compare" data-toggle="tooltip" title="{{ button_compare }}" onclick="compare.add('{{ product.product_id }}');"><i class="fa fa-exchange"></i></button>         
            </div>

            </div>
          </div>
        </div>

          </div>
        </div>
        {% endfor %}
    </div>
 </div> {% endif %}

