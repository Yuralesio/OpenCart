</div>

<script type="text/javascript" >
$( document ).ready(function() {
  var latest= $('#hor_latest');
  var amountHeaderImages = latest.find('img').length;
  latest.owlCarousel({
    // loop:true,
    nav:true,
    margin:15,
    responsiveClass:true,
    responsive:{
        0:{
            items:1,
            loop: (amountHeaderImages > 1), // if only 1 item no loop
        },
        600:{
            items:3,
            loop: (amountHeaderImages > 3), // if only 1 item no loop
        },
        1000:{
            items:4,
            loop: (amountHeaderImages > 4),// if only 1 item no loop
        }
    }
});
